{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "functions": {
        "app/main.py": {
            "maxDuration": 60
        }
    },
    "env": {
        "PYTHON_VERSION": "3.11"
    }
}